{
  "extends": ["tslint:latest", "tslint-config-airbnb", "tslint-react"],
  "rules": {
    "trailing-comma": [true, {
      "singleline": "never",
      "multiline": "never",
      "esSpecCompliant": true
    }],
    "no-unused-variable": true,
    "ordered-imports": [
      true,
      {
        "import-sources-order": "lowercase-last",
        "named-imports-order": "lowercase-last"
      }
    ]
  }
}
